from selenium.webdriver.chrome import webdriver
from selenium.webdriver.common.by import By


class DemoSearch():
    def get_demo_search(self):
        driver = webdriver.Chrome(executable_path="")
        driver.get("https://www.yatra.com/")

        Search= driver.find_element(By.XPATH, "//input[@id='BE_flight_origin_city']")
        driver.execute_script("arguments[0],click(Search)")
        var1 = driver.find_element(By.XPATH, "//input[@id='BE_flight_origin_city']").Send_keys

ddmemoSearch=DemoSearch()
DemoSearch.get_demo_search()
